              },
              "specGain": {
                  "attr": "Ks",
                  "channel": "r",
                  "colorspace": "Raw"
              },
              "specColor": {
                  "attr": "KsColor",
                  "colorspace": "Raw",
                  "name_match": ["Specular", "spec", "specular", "specColor"]
              },
              "specRough": {
                  "attr": "specularRoughness",
                  "channel": "r",
                  "colorspace": "Raw",
                  "name_match": ["Roughness"]
              },
              "normal": {
                  "attr": "normalCamera",
                  "normal": "True",
                  "colorspace": "sRGB",
                  "name_match": [
                      "Normal"
